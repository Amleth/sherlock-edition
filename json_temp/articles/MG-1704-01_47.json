":"/div/quote/#[41]/lg/l[2]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/#[41]/lg/l[3]/#"},"hi":{"@rend":"i","#":{"value":"Daigne éterniser son bonheur.","id":"/div/quote/#[41]/lg/l[3]/hi/#"}}}]}},{"ab":{"@type":"ornament","#":{"value":"***","id":"/div/quote/#[42]/ab/#"}}},{"lg":{"l":[{"#":[{"#":{"value":" ","id":"/div/quote/#[43]/lg/l[0]/#[0]/#"}},{"hi":{"@rend":"i","#":{"value":"Philippe","id":"/div/quote/#[43]/lg/l[0]/#[1]/hi/#"}}},{"note":{"@resp":"author","@place":"below","p":{"value":"Mr le Marquis de Dangeau.","id":"/div/quote/#[43]/lg/l[0]/#[2]/note/p"}}},{"hi":{"@rend":"i","#":{"value":"est nostre Chef illustre","id":"/div/quote/#[43]/lg/l[0]/#[3]/hi/#"}}}]},{"hi":{"@rend":"i","#":{"value":"Redouble nostre ardeur à marcher sur ses pas ;","id":"/div/quote/#[43]/lg/l[1]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/#[43]/lg/l[2]/#"},"hi":{"@rend":"i","#":{"value":"Fais que toûjours d’un nouveau lustre","id":"/div/quote/#[43]/lg/l[2]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/#[43]/lg/l[3]/#"},"hi":{"@rend":"i","#":{"value":"Son nom brille jusqu’au trépas.","id":"/div/quote/#[43]/lg/l[3]/hi/#"}}}]}},{"ab":{"@type":"ornament","#":{"value":"***","id":"/div/quote/#[44]/ab/#"}}},{"lg":{"l":[{"#":{"value":" ","id":"/div/quote/#[45]/lg/l[0]/#"},"hi":{"@rend":"i","#":{"value":"Destructeur de l’Idolâtrie ;","id":"/div/quote/#[45]/lg/l[0]/hi/#"}}},{"hi":{"@rend":"i","#":{"value":"Fais-nous cuëillir aussi des lauriers immortels","id":"/div/quote/#[45]/lg/l[1]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/#[45]/lg/l[2]/#"},"hi":{"@rend":"i","#":{"value":"Pour la gloire de la Patrie,","id":"/div/quote/#[45]/lg/l[2]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/#[45]/lg/l[3]/#"},"hi":{"@rend":"i","#":{"value":"Et pour le soûtien des Autels.","id":"/div/quote/#[45]/lg/l[3]/hi/#"}}}]}}]}}}
{"div":{"@xmlns":"http://www.tei-c.org/ns/1.0","@type":"article","@xml:id":"MG-1703-05_372","head":{"value":"Enigme","id":"/div/head"},"bibl":{"title":{"value":"Mercure galant","id":"/div/bibl/title"},"#":{"value":", mai 1703 [tome 5], p. 372-373.","id":"/div/bibl/#"}},"p":{"#1":{"value":"L’Enigme qui suit est de M","id":"/div/p/#1"},"hi":{"@rend":"sup","#":{"value":"r","id":"/div/p/hi/#"}},"#2":{"value":" Daubicourt.","id":"/div/p/#2"}},"quote":{"label":{"value":"ENIGME.","id":"/div/quote/label"},"lg":{"l":[{"hi":{"@rend":"i","#":{"value":"La taille courte à large pance","id":"/div/quote/lg/l[0]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/lg/l[1]/#"},"hi":{"@rend":"i","#":{"value":"Tout pesant qu’est mon corps, il est des plus dispos,","id":"/div/quote/lg/l[1]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/lg/l[2]/#"},"hi":{"@rend":"i","#":{"value":"Je voltige &amp; fais mille sauts","id":"/div/quote/lg/l[2]/hi/#"}}},{"hi":{"@rend":"i","#":{"value":"Et j’observe en dansant une juste cadence.","id":"/div/quote/lg/l[3]/hi/#"}}},{"hi":{"@rend":"i","#":{"value":"Si l’éclat de mes airs chagrine un endormy","id":"/div/quote/lg/l[4]/hi/#"}}},{"hi":{"@rend":"i","#":{"value":"Je fais souvent plaisir au studieux qui veille.","id":"/div/quote/lg/l[5]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/lg/l[6]/#"},"hi":{"@rend":"i","#":{"value":"Le diligent est mon ami,","id":"/div/quote/lg/l[6]/hi/#"}}},{"hi":{"@rend":"i","#":{"value":"Et je suis incommode où regne la bouteille.","id":"/div/quote/lg/l[7]/hi/#"}}}]}}}}
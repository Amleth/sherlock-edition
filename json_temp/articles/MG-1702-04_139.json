{"div":{"@xmlns":"http://www.tei-c.org/ns/1.0","@type":"article","@xml:id":"MG-1702-04_139","head":{"value":"[Madrigal]","id":"/div/head"},"bibl":{"title":{"value":"Mercure galant","id":"/div/bibl/title"},"#":{"value":", avril 1702 [tome 5], p. 139.","id":"/div/bibl/#"}},"p":{"#1":{"value":"M","id":"/div/p/#1"},"hi":{"@rend":"sup","#":{"value":"r","id":"/div/p/hi/#"}},"#2":{"value":" Tesson est Autheur du Madrigal que j’ajoute icy.","id":"/div/p/#2"}},"quote":{"lg":{"l":[{"#":{"value":" ","id":"/div/quote/lg/l[0]/#"},"hi":{"@rend":"i","#":{"value":"Iris, que vous a fait mon cœur","id":"/div/quote/lg/l[0]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/lg/l[1]/#"},"hi":{"@rend":"i","#":{"value":"Pour le condamner au silence ?","id":"/div/quote/lg/l[1]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/lg/l[2]/#"},"hi":{"@rend":"i","#":{"value":"Est-ce pour avoir trop d’ardeur","id":"/div/quote/lg/l[2]/hi/#"}}},{"hi":{"@rend":"i","#":{"value":"Que vous le contraignez à cacher ce qu’il pense,","id":"/div/quote/lg/l[3]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/lg/l[4]/#"},"hi":{"@rend":"i","#":{"value":"Ah ! le tendre aveu de mes feux","id":"/div/quote/lg/l[4]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/lg/l[5]/#"},"hi":{"@rend":"i","#":{"value":"A sans doute allarmé vôtre ame,","id":"/div/quote/lg/l[5]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/lg/l[6]/#"},"hi":{"@rend":"i","#":{"value":"Et pour cacher vôtre naissante flame,","id":"/div/quote/lg/l[6]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/lg/l[7]/#"},"hi":{"@rend":"i","#":{"value":"Vous le forcez à ce silence affreux.","id":"/div/quote/lg/l[7]/hi/#"}}},{"hi":{"@rend":"i","#":{"value":"Cessons il en est temps, Iris, de nous contraindre,","id":"/div/quote/lg/l[8]/hi/#"}}},{"hi":{"@rend":"i","#":{"value":"De mes feux innocens vous n’avez rien à craindre ;","id":"/div/quote/lg/l[9]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/lg/l[10]/#"},"hi":{"@rend":"i","#":{"value":"Quant un Amant joint l’estime à l’amour","id":"/div/quote/lg/l[10]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/lg/l[11]/#"},"hi":{"@rend":"i","#":{"value":"Doit-on craindre un tendre retour ?","id":"/div/quote/lg/l[11]/hi/#"}}}]}}}}
:{"@rend":"i","#":{"value":"Pour expirer sur une Croix.","id":"/div/quote/#[29]/lg/l[3]/hi/#"}}}]}},{"ab":{"@type":"ornament","#":{"value":"***","id":"/div/quote/#[30]/ab/#"}}},{"lg":{"l":[{"#":{"value":" ","id":"/div/quote/#[31]/lg/l[0]/#"},"hi":{"@rend":"i","#":{"value":"Mais helas ! dans cette Ninive","id":"/div/quote/#[31]/lg/l[0]/hi/#"}}},{"hi":{"@rend":"i","#":{"value":"Du Prophete Jonas tu n’auras pas le sort ;","id":"/div/quote/#[31]/lg/l[1]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/#[31]/lg/l[2]/#"},"hi":{"@rend":"i","#":{"value":"Et pour prix d’une Foy si vive,","id":"/div/quote/#[31]/lg/l[2]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/#[31]/lg/l[3]/#"},"hi":{"@rend":"i","#":{"value":"Tu dois n’attendre que la mort.","id":"/div/quote/#[31]/lg/l[3]/hi/#"}}}]}},{"ab":{"@type":"ornament","#":{"value":"***","id":"/div/quote/#[32]/ab/#"}}},{"lg":{"l":[{"#":{"value":" ","id":"/div/quote/#[33]/lg/l[0]/#"},"hi":{"@rend":"i","#":{"value":"Ta Foy te destine au supplice,","id":"/div/quote/#[33]/lg/l[0]/hi/#"}}},{"hi":{"@rend":"i","#":{"value":"Et parmi les Martirs te marque un noble rang ?","id":"/div/quote/#[33]/lg/l[1]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/#[33]/lg/l[2]/#"},"hi":{"@rend":"i","#":{"value":"Ouy, tu bâtis un édifice,","id":"/div/quote/#[33]/lg/l[2]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/#[33]/lg/l[3]/#"},"hi":{"@rend":"i","#":{"value":"Qu’il faut cimenter de ton sang.","id":"/div/quote/#[33]/lg/l[3]/hi/#"}}}]}},{"ab":{"@type":"ornament","#":{"value":"***","id":"/div/quote/#[34]/ab/#"}}},{"lg":{"l":[{"#":{"value":" ","id":"/div/quote/#[35]/lg/l[0]/#"},"hi":{"@rend":"i","#":{"value":"O Ciel ! quelle horrible tempeste !","id":"/div/quote/#[35]/lg/l[0]/hi/#"}}},{"hi":{"@rend":"i","#":{"value":"La Terre, les Enfers, tout s’arme contre toy ;","id":"/div/quote/#[35]/lg/l[1]/hi/#"}}},{"#":{"value":" ","id":"/div/quote/#[35]/lg/l[2]/#"},"hi":{"@rend":"i","#":{"value":"Et tu livres enfin ta teste","id":"/div/quote/#[35]/lg/l[2]/hi/#"}}},{"#":{"valu
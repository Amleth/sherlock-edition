":"i","#":{"value":"Nous sommes par tout arrestez,","id":"/div/quote/#[9]/lg/l[2]/#"}},{"@rend":"i","#":{"value":" Rien n'échape à Tourville.","id":"/div/quote/#[9]/lg/l[3]/#"}},{"@rend":"i","#":{"value":"Helas ! en vain nous le fuyons,","id":"/div/quote/#[9]/lg/l[4]/#"}},{"@rend":"i","#":{"value":"Il nous brûle &amp; nous coule à fonds.","id":"/div/quote/#[9]/lg/l[5]/#"}}]}},{"ab":{"@type":"ornament","#":{"value":"***","id":"/div/quote/#[10]/ab/#"}}},{"lg":{"l":[{"@rend":"i","#":{"value":"Ah ! que LOUIS est bien servy","id":"/div/quote/#[11]/lg/l[0]/#"}},{"@rend":"i","#":{"value":" Sur la Terre &amp; sur l'Onde !","id":"/div/quote/#[11]/lg/l[1]/#"}},{"@rend":"i","#":{"value":"Chacun le veut rendre à l'envy","id":"/div/quote/#[11]/lg/l[2]/#"}},{"@rend":"i","#":{"value":" Le plus puissant du Monde.","id":"/div/quote/#[11]/lg/l[3]/#"}},{"@rend":"i","#":{"value":"Helas ! quelle rage pour nous","id":"/div/quote/#[11]/lg/l[4]/#"}},{"@rend":"i","#":{"value":"Quand nous voulons l'accabler tous !","id":"/div/quote/#[11]/lg/l[5]/#"}}]}},{"ab":{"@type":"ornament","#":{"value":"***","id":"/div/quote/#[12]/ab/#"}}},{"lg":{"l":[{"@rend":"i","#":{"value":"Que nous sert de voir aujourd'huy","id":"/div/quote/#[13]/lg/l[0]/#"}},{"@rend":"i","#":{"value":" Toute l'Europe en Ligues ?","id":"/div/quote/#[13]/lg/l[1]/#"}},{"@rend":"i","#":{"value":"Rien ne réüssit contre luy,","id":"/div/quote/#[13]/lg/l[2]/#"}},{"@rend":"i","#":{"value":" Il rompt toutes nos brigues.","id":"/div/quote/#[13]/lg/l[3]/#"}},{"@rend":"i","#":{"value":"Helas ! tout ce qu'on entreprend","id":"/div/quote/#[13]/lg/l[4]/#"}},{"@rend":"i","#":{"value":"Ne sert qu'à le rendre plus grand.","id":"/div/quote/#[13]/lg/l[5]/#"}}]}},{"ab":{"@type":"ornament","#":{"value":"***","id":"/div/quote/#[14]/ab/#"}}},{"lg":{"l":[{"@rend":"i","#":{"value":"Voy tous les maux que tu nous fais,","id":"/div/quote/#[15]/lg/l[0]/#"}},{"@rend":"i","#":{"value":" Maudit Prince d'Orange ;","id":"/div/quote/#[15]/lg/l[1]/#"}},{"@rend":"i",